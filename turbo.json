{
	"$schema": "https://turbo.build/schema.json",
	"pipeline": {
		"build": {
			"dependsOn": ["^lint", "^db:generate", "^build"],
			"outputs": [".svelte-kit/**", "dist", "build"]
		},
		"lint": {},
		"dev": {
			"dependsOn": ["^db:generate"],
			"cache": false,
			"persistent": true
		},
		"db:generate": {
			"cache": false
		},
		"db:push": {
			"cache": false
		}
	}
}
