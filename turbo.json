{
	"$schema": "https://turbo.build/schema.json",
	"pipeline": {
		"build": {
			"dependsOn": ["^lint", "^build"],
			"outputs": [".svelte-kit/**", "dist", "build"],
			"dotEnv": [".env"]
		},
		"lint": {},
		"dev": {
			"dependsOn": ["^db:generate"],
			"cache": false,
			"persistent": true,
			"dotEnv": [".env.dev"]
		},
		"db:generate": {
			"cache": false
		},
		"db:push": {
			"cache": false
		}
	}
}
